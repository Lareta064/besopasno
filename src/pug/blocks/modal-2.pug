.modal-frame(frame-modal="enter-object-data")
	.modal-close.btn-close(frame-close)
		svg.svg-close
			use(xlink:href="../img/svg-sprite.svg#svgClose")
	.modal-frame-content
		.container-slim
			.modal-frame__img
				.success-check
					svg.svg-success
						use(xlink:href="../img/svg-sprite.svg#svgSuccess")
			.modal-frame__text
				.title-24 Ввести данные о объекте и собственнике сейчас?

	.modal-frame__footer
		.container-slim
			+button('button', '', 'Да, конечно', 'button--blue')
			+button('button', '', 'Не сейчас', 'button--white')(frame-close)