									+double-button('Заказать проверку', '2 999 ₽')(class="fz-16")
						
					//======tabs content ======
					.custom-tabs__content
						//=============== 1 ============
						.custom-tabs__content-block(ct-content="1")
							include ../blocks/report-report
						//=============== 2 ============
						.custom-tabs__content-block(ct-content="2")
							include ../blocks/report-courts
						//=============== 3 ============
						.custom-tabs__content-block(ct-content="3")
							include ../blocks/report-arbitrage
						//=============== 4 ============
						.custom-tabs__content-block(ct-content="4")
							include ../blocks/report-bankrot
						//=============== 5 ============
						.custom-tabs__content-block(ct-content="5")
